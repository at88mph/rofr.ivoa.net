<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
          PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
          "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

   <display-name>Registry Harvest Validator</display-name>

   <servlet>
      <servlet-name>HarvestValidater</servlet-name>
      <servlet-class>net.ivoa.registry.validate.HarvestValidaterWebApp</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>VOResourceValidater</servlet-name>
      <servlet-class>net.ivoa.registry.validate.VOResourceValidaterWebApp</servlet-class>
   </servlet>

   <servlet-mapping>
      <servlet-name>HarvestValidater</servlet-name>
      <url-pattern>/HarvestValidater</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>VOResourceValidater</servlet-name>
      <url-pattern>/VOResourceValidater</url-pattern>
   </servlet-mapping>
</web-app>
